@attribute [Authorize(Roles = Roles.AdminRole)]

<!--begin::Sidebar menu-->
<div id="#kt_app_sidebar_menu" data-kt-menu="true" data-kt-menu-expand="false" class="flex-column-fluid menu menu-sub-indention menu-column menu-rounded menu-active-bg mb-7">
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/" MenuTitle="Dashboard" Icon="@Icons.Material.TwoTone.SpaceDashboard" NavLinkMatch="@NavLinkMatch.All" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/ReferenceData" MenuTitle="Reference Data" Icon="@Icons.Material.TwoTone.ManageSearch" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <div data-kt-menu-trigger="click" class="menu-item here menu-accordion">
        <!--begin:Menu link-->
        <span class="menu-link">
            <span class="menu-icon">
                <MudIcon Color="@(IsDarkMood? Color.Info : Color.Dark)" Size="Size.Large" Icon="@Icons.Material.TwoTone.Security" Title="SecurityLogs" />
            </span>
            <span class="menu-title">@languageContainer.Keys["Security Logs"]</span>
            <span class="menu-arrow"></span>
        </span>
        <!--end:Menu link-->
        <!--begin:Menu sub-->
        <div class="menu-sub menu-sub-accordion">
            <!--begin:Menu item-->
            <Navlink IsDarkMood="@IsDarkMood" Href="/Audits" MenuTitle="Audits" Icon="@Icons.Material.TwoTone.History" Size="@Size.Medium" />
            <!--end:Menu item-->
            <!--begin:Menu item-->
            <Navlink IsDarkMood="@IsDarkMood" Href="/LoginLogs" MenuTitle="Login Logs" Icon="@Icons.Material.TwoTone.SafetyCheck" Size="@Size.Medium" />
            <!--end:Menu item-->
        </div>
        <!--end:Menu sub-->
    </div>
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <div data-kt-menu-trigger="click" class="menu-item here menu-accordion">
        <!--begin:Menu link-->
        <span class="menu-link">
            <span class="menu-icon">
                <MudIcon Color="@(IsDarkMood? Color.Info : Color.Dark)" Size="Size.Large" Icon="@Icons.Material.TwoTone.Apartment" Title="Company" />
            </span>
            <span class="menu-title">@languageContainer.Keys["Company"]</span>
            <span class="menu-arrow"></span>
        </span>
        <!--end:Menu link-->
        <!--begin:Menu sub-->
        <div class="menu-sub menu-sub-accordion">
            <!--begin:Menu item-->
            <Navlink IsDarkMood="@IsDarkMood" Href="/Managers" MenuTitle="Managers" Icon="@Icons.Material.TwoTone.Diversity3" Size="@Size.Medium" />
            <!--end:Menu item-->
            <!--begin:Menu item-->
            <Navlink IsDarkMood="@IsDarkMood" Href="/Supervisors" MenuTitle="Supervisors" Icon="@Icons.Material.TwoTone.SupervisorAccount" Size="@Size.Medium" />
