    <Navlink IsDarkMood="@IsDarkMood" Href="/Quotations" MenuTitle="Quotations" Icon="@EntityIcons.QuotationsIcon" />
    <!--end:Menu item-->
</div>
<!--end::Sidebar menu-->
@code {
    [Parameter][EditorRequired] public bool IsDarkMood { get; set; }
}
