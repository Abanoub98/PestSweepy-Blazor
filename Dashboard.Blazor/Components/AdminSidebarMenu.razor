            <!--begin:Menu item-->
            <Navlink IsDarkMood="@IsDarkMood" Href="/Providers" MenuTitle="Providers" Icon="@Icons.Material.TwoTone.Engineering" Size="@Size.Medium" />
            <!--end:Menu item-->
        </div>
        <!--end:Menu sub-->
    </div>
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/CompanyInfo" MenuTitle="Company Info" Icon="@EntityIcons.CompanyInfoIcon" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Reviews" MenuTitle="Reviews" Icon="@EntityIcons.ReviewIcon" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Coupons" MenuTitle="Coupons" Icon="@EntityIcons.CouponIcon" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Categories" MenuTitle="Categories" Icon="@Icons.Material.TwoTone.Category" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Services" MenuTitle="Services" Icon="@Icons.Material.TwoTone.Handyman" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Clients" MenuTitle="Clients" Icon="@Icons.Material.TwoTone.Diversity1" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/ContractClients" MenuTitle="Contract Clients" Icon="@Icons.Material.TwoTone.Diversity1" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Contracts" MenuTitle="Contracts" Icon="@EntityIcons.ContractsIcon" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Payments" MenuTitle="Payments" Icon="@Icons.Material.Filled.Payment" />
    <!--end:Menu item-->
    <!--begin:Menu item-->
    <Navlink IsDarkMood="@IsDarkMood" Href="/Currencies" MenuTitle="Currencies" Icon="@EntityIcons.CurrencyIcon" />
    <!--end:Menu item-->
